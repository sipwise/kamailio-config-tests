sip_in:
  - '^BYE sip:127.0.0.1:5080(;prxroute=1)? SIP/2.0'
  - 'Record-Route: <sip:127.0.0.1;lr=on;ftag=[^;]+;ngcplb=yes;socket=udp:127.0.0.1:5060>'
  - 'From: <sip:[% scenarios.1.username %]@invite-callforward.scenarios.test>;tag=[\w-]+'
  - 'To: <sip:[% scenarios.1.responders.0.username %]@invite-callforward.scenarios.test>;tag=[\w-]+'
  - 'CSeq: \d+ BYE'
  - 'Contact: <?sip:[% scenarios.1.username %]@[% scenarios.1.ip %]:[% scenarios.1.port %](;[^>]+)?>?'
  - 'Content-Length: 0'
  - 'P-NGCP-Src-Ip: [% scenarios.1.ip %]'
  - 'P-NGCP-Src-Port: [% scenarios.1.port %]'
  - 'P-NGCP-Src-Proto: udp'
  - 'P-NGCP-Src-Af: 4'
  - 'P-Sock-Info: udp:127.0.0.1:5060'
  - 'P-LB-Uptime: \d+'
sip_out:
  - [
      'SIP/2.0 100 Trying',
      'From: <sip:[% scenarios.1.username %]@invite-callforward.scenarios.test>;tag=[\w-]+',
      'To: <sip:[% scenarios.1.responders.0.username %]@invite-callforward.scenarios.test>;tag=[\w-]+',
      'CSeq: \d+ BYE',
      'P-Out-Socket: udp:127.0.0.1:5060',
      'P-NGCP-Auth-IP: [% scenarios.0.ip %]',
      'P-NGCP-Auth-UA: <null>',
      'Server: Sipwise NGCP Proxy',
      'Content-Length: 0',
    ]
  - [
      'BYE sip:127.0.0.1:5080;prxroute=1 SIP/2.0',
      'Record-Route: <sip:127.0.0.1:5062;lr=on;ftag=[^;]+>',
      'Record-Route: <sip:127.0.0.1;lr=on;ftag=[^;]+;ngcplb=yes;socket=udp:127.0.0.1:5060>',
      'From: <sip:[% scenarios.1.username %]@invite-callforward.scenarios.test>;tag=[\w-]+',
      'To: <sip:[% scenarios.1.responders.0.username %]@invite-callforward.scenarios.test>;tag=[\w-]+',
      'CSeq: \d+ BYE',
      'Contact: <?sip:[% scenarios.1.username %]@[% scenarios.1.ip %]:[% scenarios.1.port %](;[^>]+)?>?',
      'Content-Length: 0',
      'P-LB-Uptime: \d+',
      'P-Asserted-Identity: <sip:[% invite_callforward_scenarios_test.testuser1002.phone_number %]@invite-callforward.scenarios.test>',
    ]
