sip_in:
  - '^INVITE sip:[% scenarios.1.responders.0.username %]@'
  - 'Contact: <sip:[% scenarios.1.username %]@[% scenarios.1.ip %]:[% scenarios.1.port %](;[^>]+)?>'
  - 'CSeq: 2 INVITE'
  - 'Max-Forwards: 69'
  - 'Content-Type: application/sdp'
  - 'Proxy-Authorization: Digest username="[% scenarios.1.username %]"'
sip_out:
  - [
      '^SIP/2.0 100 Trying',
      'CSeq: 2 INVITE',
      'From: <sip:[% scenarios.0.responders.0.username %]@',
      'To: <sip:[% scenarios.1.responders.0.username %]@'
    ]
  - [
      '^SIP/2.0 101 Connecting',
      'CSeq: 2 INVITE',
      'From: <sip:[% scenarios.0.responders.0.username %]@',
      'To: <sip:[% scenarios.1.responders.0.username %]@'
    ]
  - [
      '^SIP/2.0 403 Caller locked',
      'CSeq: 2 INVITE',
      'From: <sip:[% scenarios.0.responders.0.username %]@',
      'To: <sip:[% scenarios.1.responders.0.username %]@'
    ]
