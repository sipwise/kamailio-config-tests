sip_in:
  - '^INVITE sip:[% invite_offline_scenarios_test.testuser1003.phone_number %]@'
  - 'Contact: <sip:[% scenarios.0.username %]@[% scenarios.0.ip %]:[% scenarios.0.port %](;[^>]+)?>'
  - 'CSeq: 3 INVITE'
  - 'Max-Forwards: 69'
  - 'Content-Type: application/sdp'
  - 'Proxy-Authorization: Digest username="[% scenarios.0.username %]"'
sip_out:
  - [
      '^SIP/2.0 100 Trying',
      'CSeq: 3 INVITE',
      'From: <sip:[% scenarios.0.username %]@'
    ]
  - [
      '^SIP/2.0 101 Connecting',
      'CSeq: 3 INVITE',
      'From: <sip:[% scenarios.0.username %]@'
    ]
  - [
      '^SIP/2.0 480 Offline',
      'CSeq: 3 INVITE',
      'From: <sip:[% scenarios.0.username %]@'
    ]
