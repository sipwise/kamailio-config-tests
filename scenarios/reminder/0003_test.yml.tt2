sip_in:
  - '^INVITE'
  - 'Contact: sip:testuser1001@'
  - 'CSeq: 2 INVITE'
  - 'Max-Forwards: 69'
  - 'Content-Type: application/sdp'
  - 'Proxy-Authorization: Digest username="testuser1001"'
sip_out:
  - [
      '^SIP/2.0 100 Trying',
      'CSeq: 2 INVITE',
      'From: <sip:testuser1001@',
      'To: <sip:\*55\*\d{4}@'
    ]
  - [
      '^SIP/2.0 101 Connecting',
      'CSeq: 2 INVITE',
      'From: <sip:testuser1001@',
      'To: <sip:\*55\*\d{4}@'
    ]
  - [
      '^INVITE sip:\*55\*\d{4}@',
      'CSeq: 2 INVITE',
      'From: <sip:4311001@',
      'To: <sip:\*55\*\d{4}@',
      'Content-Type: application/sdp',
      'P-Caller-UUID: \w+',
      'P-Callee-UUID: \w+',
      'P-App-Name: sw_vsc'
    ]
  - [
      '^SIP/2.0 200 OK',
      'CSeq: 2 INVITE',
      'From: <sip:testuser1001@',
      'To: <sip:\*55\*\d{4}@',
      'Content-Type: application/sdp'
    ]
